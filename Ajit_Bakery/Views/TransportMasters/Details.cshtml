@model Ajit_Bakery.Models.TransportMaster

@{
    ViewData["Title"] = "Details";
}

<div class="card">

    <div class="card-header bg-dark text-white py-2">
        <div class="d-flex align-items-baseline">
            <a asp-action="Index" asp-controller="TransportMasters" class="btn btn-link text-white"><i class="fa fa-arrow-left"></i></a>
            <h4 class="mx-auto text-white text-2xl font-semibold"> Transport Master - Detailes </h4>
            @*  <a asp-action="Create" class="btn btn-success btn-sm text-white">
                <i class="fas fa-plus-square"></i> Packaging
            </a> *@
        </div>
    </div>
    <div class="card-body mt-4">

        <dl class="row">
            <dt class="col-sm-2">
                Driver Name
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DriverName)
            </dd>
            <dt class="col-sm-2">
                Driver Contact No.
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DriverContactNo)
            </dd>
            <dt class="col-sm-2">
                Vehicle No.
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.VehicleNo)
            </dd>
            <dt class="col-sm-2">
                Vehicle Own
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.VehicleOwn)
            </dd>
            <dt class="col-sm-2">
                Create Date
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CreateDate)
            </dd>

            <dt class="col-sm-2">
                Modified Date
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ModifiedDate)
            </dd>

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.User)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.User)
            </dd>
        </dl>
    </div>
</div>







